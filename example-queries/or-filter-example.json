{
  "datasetId": "events",
  "kpis": [
    "invited_suppliers_count",
    "offered_suppliers_count",
    "rejected_suppliers_count",
    "viewed_suppliers_count"
  ],
  "groupBy": [
    "event_number",
    "event_name",
    "created_by",
    "state_name",
    "submission_deadline"
  ],
  "filterGroups": [
    {
      "logic": "or",
      "filters": [
        {
          "field": "created_by",
          "operator": "contains",
          "value": ["Koch"]
        },
        {
          "field": "created_by",
          "operator": "contains",
          "value": ["Adil"]
        }
      ]
    }
  ],
  "filters": [
    {
      "field": "event_type",
      "operator": "equals",
      "value": ["RFQ"]
    },
    {
      "field": "deadline",
      "operator": "inDateRange",
      "value": ["2025-01-01", "2025-04-01"]
    }
  ],
  "sort": {
    "by": "submission_deadline",
    "direction": "desc"
  },
  "page": {
    "limit": 50,
    "offset": 0
  }
}
